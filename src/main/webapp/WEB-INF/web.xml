<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
  	<listener-class>com.yanan.frame.servlets.ServletContextInit</listener-class>
  </listener>
  <servlet>
  	<servlet-name>plugin.mvc</servlet-name>
  	<servlet-class>com.yanan.frame.servlets.CoreDispatcher</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>plugin.mvc</servlet-name>
  	<url-pattern>/*</url-pattern>
  </servlet-mapping>
</web-app>
